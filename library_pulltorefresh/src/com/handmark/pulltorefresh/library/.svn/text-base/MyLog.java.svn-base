package com.handmark.pulltorefresh.library;

import android.util.Log;

public class MyLog {
	static boolean isDebug = false;

	public static void d(String tag, String msg) {
		if (isDebug)
			Log.d(tag, msg);
	}

	public static void w(String logTag, String string) {
		if (isDebug)
			Log.w(logTag, string);

	}
}
